extends base.pug

block main
    section(class='page')
        .container
            .page__body
                h2.page__title Почти все готово

                form(class='form' method='post' enctype='multipart/form-data')
                    {% csrf_token %}
                    .form__error {{ form.non_field_errors }}
                    div(class='cols-auto-fit')
                        div
                            .form__row
                                .checkbox-field
                                    label(for='{{ form.is_owner_shelter.id_for_label }}') {{ form.is_owner_shelter.label }}
                                    {{ form.is_owner_shelter }}
                        div
                            .form__row
                                .form__image-preview
                                    img(id='zoo-image-preview' src='data:image/png;base64,')
                            .form__row
                                div(class='form__select-photo select-photo' data-for='zoo-image-preview')
                                    label(for='{{form.photo.id_for_label}}')
                                        {{form.photo}}
                                        button(class='btn' type='button') Фото
                                        span.select-photo__url Какой-то путь...
                    .form__footer
                        button(class='btn' type='submit') Дальше